{
  "name": "api",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "description": "REST API for Waves",
  "repository": {
    "url": ""
  },
  "keywords": [
    "node",
    "express",
    "typescript",
    "api"
  ],
  "main": "dist/index.js",
  "scripts": {
    "docker:build": "docker build -t api .",
    "docker:compose": "docker-compose up -d --build",
    "docker:run": "docker run -p 8080:8080 api",
    "format": "pnpm run prettier && pnpm run lint",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts,.tsx --quiet --fix",
    "dev": "pnpm run docker:compose && bob-tsm --node-env=dev --cjs --watch=src src/index.ts",
    "test": "cross-env jest",
    "start": "bob-tsm --node-env=prod --cjs src/index.ts"
  },
  "dependencies": {
    "@ethereum-attestation-service/eas-sdk": "^1.1.0-beta.3",
    "@urql/core": "4.1.2",
    "bob-tsm": "^1.1.2",
    "connect-redis": "7.1.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "helmet": "7.0.0",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "morgan": "1.10.0",
    "node-fetch": "^3.3.1",
    "redis": "4.6.8",
    "siwe": "2.1.4",
    "tap": "^16.3.4"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/express-session": "1.17.7",
    "@types/node-fetch": "2.5.12",
    "@types/supertest": "2.0.12",
    "@types/tap": "^15.0.8",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.12",
    "supertest": "6.3.3"
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testEnvironment": "node"
  }
}
